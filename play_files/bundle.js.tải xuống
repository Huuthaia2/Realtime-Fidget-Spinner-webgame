        Ct = new (function () {
            function e() {
                l(this, e)
            }

            return c(e, [{
                key: "init",
                value: function () {
                    this.curShopTime = new Date, this.lastRefreshTime = new Date, this.lastResetShopRefreshTime = new Date, this.initLastRefreshGoods(), Laya.timer.loop(1e3, this, this.updateShopTime)
                }
            }, {
                key: "initLastRefreshGoods",
                value: function () {
                    var e = +Laya.LocalStorage.getItem("refreshShopTime");
                    !e || this.checkIsNextDay(e) ? this.resetShopRefreshTime() : (this.lastResetShopRefreshTime.setTime(e), this.curGoodRefreshed = Laya.LocalStorage.getJSON("refreshShopTimes"));
                    var t = Laya.LocalStorage.getJSON("refreshGoods"),
                        i = !1;
                    t && !this.checkGoodsIsRefresh() || (i = !0), i ? this.updateGoodsInfo() : (this.lastRefreshGoods = t.goods, this.lastRefreshTime.setTime(t.time))
                }
            }, {
                key: "checkIsNextDay",
                value: function (e) {
                    var t = new Date;
                    return t.setTime(e), t.getFullYear() < this.curShopTime.getFullYear() || t.getUTCMonth() < this.curShopTime.getUTCMonth() || t.getUTCDate() < this.curShopTime.getUTCDate()
                }
            }, {
                key: "resetShopRefreshTime",
                value: function () {
                    this.lastResetShopRefreshTime.setTime(864e5 * Math.floor(this.curShopTime.getTime() / 864e5)), this.curGoodRefreshed = {
                        free_times: 0,
                        ad_times: 0,
                        coin_times: 0
                    }, Laya.LocalStorage.setItem("refreshShopTime", this.lastResetShopRefreshTime.getTime() + ""), Laya.LocalStorage.setJSON("refreshShopTimes", this.curGoodRefreshed), g.dispatch(f.UPDATE_SHOP_REFRESH_TIMES)
                }
            }, {
                key: "updateGoodsBuy",
                value: function (e) {
                    this.lastRefreshGoods[e] = Math.max(this.lastRefreshGoods[e] + 1, 1), Laya.LocalStorage.setJSON("refreshGoods", {
                        goods: this.lastRefreshGoods,
                        time: this.curShopTime.getTime()
                    })
                }
            }, {
                key: "updateShopRefreshTimes",
                value: function () {
                    var e = he.shopInfo[1];
                    return this.curGoodRefreshed.free_times < e.free_times ? (this.curGoodRefreshed.free_times += 1, Laya.LocalStorage.setJSON("refreshShopTimes", this.curGoodRefreshed), 1) : this.curGoodRefreshed.ad_times < e.ad_times ? (this.curGoodRefreshed.ad_times += 1, Laya.LocalStorage.setJSON("refreshShopTimes", this.curGoodRefreshed), 2) : this.curGoodRefreshed.coin_times < e.coin_times ? (this.curGoodRefreshed.coin_times += 1, Laya.LocalStorage.setJSON("refreshShopTimes", this.curGoodRefreshed), 3) : 0
                }
            }, {
                key: "updateShopTime",
                value: function () {
                    var e = (new Date).getTime();
                    Laya.LocalStorage.setItem("ShopTime", "".concat(e)), this.checkGoodsIsRefresh() && (this.updateGoodsInfo(), g.dispatch(f.UPDATE_SHOP_GOODS)), this.checkIsNextDay(this.lastResetShopRefreshTime.getTime()) && this.resetShopRefreshTime()
                }
            }, {
                key: "checkGoodsIsRefresh",
                value: function () {
                    return this.lastRefreshTime.getFullYear() < this.curShopTime.getFullYear() || this.lastRefreshTime.getMonth() < this.curShopTime.getMonth() || this.lastRefreshTime.getDate() < this.curShopTime.getDate() || this.lastRefreshTime.getHours() >= 0 && this.lastRefreshTime.getHours() < 12 && this.curShopTime.getHours() >= 12 && this.curShopTime.getHours() < 24 || this.lastRefreshTime.getHours() >= 12 && this.lastRefreshTime.getHours() < 24 && this.curShopTime.getHours() >= 0 && this.curShopTime.getHours() < 12
                }
            }, {
                key: "updateGoodsInfo",
                value: function () {
                    for (var e = he.shopInfo[1], t = [], i = 0, n = e.list.concat(); t.length < e.max;) {
                        0 == i && n.forEach(function (e) {
                            i += e.weight
                        });
                        for (var a = X.getRandomValue(0, 0, i), o = 0, s = 0; s < n.length; s++) {
                            var r = n[s];
                            if ((a -= r.weight) <= 0) {
                                o = s, i -= r.weight;
                                break
                            }
                        }
