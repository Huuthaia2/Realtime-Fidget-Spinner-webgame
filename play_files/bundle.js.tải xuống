                    for (var i = [], n = [], a = he.jackpotInfo[he.developInfo[e].reward[0]], o = t, s = 0; s < a.weight.length; s++)
                        for (var r = 0; r < a.weight[s]; r++) i.push(a.reward[s]);
                    for (var l = 0; l < o; l++) {
                        for (var c = Math.floor(Math.random() * i.length); !ui.developLevelInfo[i[c][0]].isUnLock && ui.developLevelInfo[i[c][0]].mod.length > 1 && i[c][2] > 0;) c = Math.floor(Math.random() * i.length);
                        n.push({
                            itemInfo: ui.developLevelInfo[i[c][0]],
                            num: i[c][1]
                        })
                    }
                    return n.sort(function () {
                        return Math.random() < .5 ? 1 : -1
                    }), n
                }
            }, {
                key: "updateOnlineTime",
                value: function () {
                    this.onlineTime += X.getDelta();
                    var e = 60 * ti.onlineTime[Math.min(this.onlineRewardGetTime, 2)];
                    this.onlineTime >= e && (this.onlineTime = e), Laya.LocalStorage.setItem("onlineTime", this.onlineTime + "")
                }
            }]), e
        }());
    di = ii || (ii = {}), di[di.level = 1] = "level", di[di.class = 2] = "class", di[di.Unlock = 3] = "Unlock", di[di.Use = 4] = "Use", ci = ni || (ni = {}), ci[ci.free = 1] = "free", ci[ci.ad = 2] = "ad", li = ai || (ai = {}), li[li.login = 1] = "login", li[li.fight = 2] = "fight", li[li.win = 3] = "win", li[li.out = 4] = "out", li[li.addLevel = 5] = "addLevel", li[li.copper = 6] = "copper", li[li.sliver = 7] = "sliver", li[li.gold = 8] = "gold", li[li.useGold = 9] = "useGold", li[li.ad = 10] = "ad", li[li.useDiamond = 11] = "useDiamond", ri = oi || (oi = {}), ri[ri.Bronze = 1] = "Bronze", ri[ri.Silver = 2] = "Silver", ri[ri.Gold = 3] = "Gold", ri[ri.Platinum = 4] = "Platinum", ri[ri.Diamond = 5] = "Diamond", ri[ri.Star = 6] = "Star", ri[ri.King = 7] = "King";
    var fi, pi = function (e) {
            function t() {
                var e;
                return l(this, t), (e = i.apply(this, arguments)).percent = 1, e.curPercent = 1, e.updateValue = .04, e
            }

            s(t, e);
            var i = r(t);
            return c(t, [{
                key: "onEnable",
