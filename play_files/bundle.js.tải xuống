                        if (this.skillReady = !0, this.playerInfo.id == ti.selfIndex && 0 == ui.playNum && !ti.isSkillPlayGuide && ti.isChooseSkillGuide && (ti.isSkillPlayGuide = !0, ti.gameView.txtGuide.visible = !0, ti.gameView.tips.text = "Release to fight", Laya.timer.once(5e3, this, function () {
