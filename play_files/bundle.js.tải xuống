                    this._topAttribute = {
                        detection: t.detection
                    }, this.action = t.action, this.specialAction = t.specialAction, this.actionIndex = -1, this.actionFinished = new Laya.Handler(this, this.nextAction)
                }
            }, {
                key: "onEnable",
                value: function () {
                    this.setAnimator(), this.setRigidbody(), this.setContainer(), this.setRotationNode(), this.setTopComponent(), this.addDirectionRing(), this.initJumpAni(), g.on(f.GAME_START, this, this.startGame)
                }
            }, {
                key: "startGame",
                value: function () {
                    this.nextAction(), this.rigidbody.isKinematic = !1
                }
            }, {
                key: "doUpdate",
                value: function () {
                    ti.gameState == Ot.runing && (this.checkDirection() ? (this.actionStatus == Ce.Skill || this.isStopMoveBySkill || this.isDead || (ie.isIosWeiXin() || this.isQuicken) && this.transformUpdate(), this.updateGradient(), this.checkSpecialAction(), this.checkSp()) : this.nextAction())
                }
            }, {
                key: "checkSkillPlay",
                value: function () {
                    this.skillReady && (this.playerSkill(this.playerInfo.ackItem.skill), 0 != this.baseSkill.info.stop_time && (this.isStopMoveBySkill = !0, Laya.timer.once(this.baseSkill.info.stop_time, this, this.stopPauseMove)), this.warInfo.curSp = 0, this.skillReady = !1)
                }
            }, {
                key: "stopPauseMove",
                value: function () {
                    this.isStopMoveBySkill = !1
                }
            }, {
                key: "updateGradient",
                value: function () {
                    var e = this.isDead ? 45 : 15;
                    if (!(this.actionStatus != Ce.Skill && this.gradient == e || this.actionStatus == Ce.Skill && this.gradient <= 0)) {
                        var t = e - this.gradient,
                            i = 2.5 * Math.abs(t) * X.getDelta() + .1,
                            n = t / Math.abs(t);
                        this.gradient += Math.min(e, n * i)
                    }
                }
            }, {
                key: "checkSpecialAction",
                value: function () {
                    if (!this.isDead && !ti.isReadyFinish && (this.specialTime += 20, this.specialTime >= 500 && (this.specialTime = 0, !this.isSpecialAction)))
                        for (var e = 0; e < this.specialAction.length; e++) {
                            var t = he.getAiActionInfo(this.specialAction[e]);
                            this.checkJudgementEnter(t.conditions) && (Laya.timer.clear(this, this.nextAction), this.doAction(t), this.isSpecialAction = !0, Laya.timer.once(t.duration, this, this.nextAction))
                        }
                }
            }, {
                key: "nextAction",
                value: function () {
                    if (!this.isDead && !ti.isReadyFinish) {
                        this.isSpecialAction && (this.isSpecialAction = !1), Laya.timer.clear(this, this.nextAction), this.actionIndex++, this.actionIndex >= this.action.length && (this.actionIndex = 0);
                        var e = this.getAction();
                        this.checkJudgementEnter(e.conditions) || (e = he.baseAction), this.doAction(e), Laya.timer.once(e.duration, this, this.nextAction)
                    }
                }
            }, {
                key: "doAction",
                value: function (e) {
                    switch (e.action) {
                        case Ce.RandomMove:
                            this.getRandomDirection();
                            break;
                        case Ce.Attack:
                            this.attackOther();
                            break;
                        case Ce.GetGood:
                            this.getGood();
                            break;
                        case Ce.Run:
                            this.run();
                            break;
                        case Ce.SideShift:
                            this.sideShift();
                            break;
                        case Ce.Skill:
                            this.playskill()
                    }
                    this.actionStatus = e.action
                }
            }, {
                key: "checkJudgementEnter",
                value: function (e) {
                    for (var t = 0; t < e.length; t++)
                        if (!this.aiJudgement(+e[t])) return !1;
                    return !0
                }
            }, {
                key: "getAction",
                value: function () {
                    var e = this.action[this.actionIndex];
                    return e.length > 1 ? he.getAiActionInfo(e[X.getRandomValue(0, 0, e.length - 1)]) : 1 == e.length ? he.getAiActionInfo(e[0]) : he.baseAction
                }
            }, {
                key: "checkDirection",
                value: function () {
                    if (this.isDead) return !0;
                    var e = new Laya.Vector3;
                    switch (this.actionStatus != Ce.RandomMove && this.actionStatus != Ce.Skill && this.actionStatus != Ce.GetGood && this.target.getComponent(Me).isDead && this.actionFinished.run(), this.actionStatus) {
                        case Ce.RandomMove:
                            e = this.targetDirection;
                            break;
                        case Ce.GetGood:
                            if (!this.target || this.target.destroyed) return !1;
                            if (!this.target.active) {
                                if (this.checkGoods(this.param0, this.param1), !this.detectedTarget) {
                                    this.actionFinished.run();
                                    break
                                }
                                this.target = this.detectedTarget, this.detectedTarget = void 0, this.targetDirection = void 0
                            }
                        case Ce.Attack:
                            if (!this.target || this.target.destroyed) return !1;
                            Laya.Vector3.subtract(this.target.transform.position, this.transform.position, e);
                            break;
                        case Ce.Run:
                            if (!this.target || this.target.destroyed) return !1;
                            Laya.Vector3.subtract(this.transform.position, this.target.transform.position, e);
                            break;
                        case Ce.SideShift:
