            function t() {
                var e;
                return l(this, t), (e = i.call(this)).bannerAd = null, e.interstitialAd = null, e.videoAd = null, e.customAd = null, e.isInterstitialLoaded = !1, e.isNativeShow = !0, e.recorder = null, e.isRecord = !1, e.recordVideoId = NaN, e.supportRecord = !0, e
            }

            s(t, e);
            var i = r(t);
            return c(t, [{
                key: "init",
                value: function () {
                    this.createVideoAd(), this.fileSystemManager = ks.getFileSystemManager(), this.getSystemInfo(), this.createRecorder()
                }
            }, {
                key: "getUserPath",
                value: function () {
                    return ks.env.USER_DATA_PATH + "/"
                }
            }, {
                key: "getSystemInfo",
                value: function () {
                    return new Promise(function (e) {
                        var t = {
                            success: function (t) {
                                t.safeArea ? ti.systemInfo = t.safeArea : ti.systemInfo = {
                                    width: t.safeArea.width,
                                    height: t.safeArea.height,
                                    top: t.safeArea.top,
                                    bottom: t.safeArea.bottom,
                                    left: t.safeArea.left,
                                    right: t.safeArea.right
                                }, e()
                            }
                        };
                        ks.getSystemInfo(t)
                    })
                }
            }, {
                key: "mkdir",
                value: function (e, t) {
                    this.fileSystemManager.mkdir({
                        dirPath: ks.env.USER_DATA_PATH + "/" + e,
                        recursive: !0,
                        success: t,
                        fail: t
                    })
                }
            }, {
                key: "rmdir",
                value: function (e, t) {
                    this.fileSystemManager.rmdir({
                        dirPath: ks.env.USER_DATA_PATH + "/" + e,
                        recursive: !0,
                        success: t,
                        fail: t
                    })
                }
            }, {
                key: "downloadFile",
                value: function (e, t, i, n) {
                    ie.Jsb.downLoadProgress && ie.Jsb.downLoadProgress.recover(), this.downLoadProgress = n, ks.downloadFile({
                        url: e,
                        timeout: 6e4,
                        success: t,
                        fail: i
                    }).onProgressUpdate(function (e) {
                        ie.Jsb.downLoadProgress && ie.Jsb.downLoadProgress.runWith(e.progress / 100)
                    })
                }
            }, {
                key: "loadSubPackages",
                value: function (e, t) {
                    return new Promise(function (i, n) {
                        ie.Jsb.subPackageProgress = t, ks.loadSubpackage({
                            name: e,
                            success: i,
                            fail: n
                        }).onProgressUpdate(function (e) {
                            ie.Jsb.subPackageProgress && ie.Jsb.subPackageProgress.runWith(e.progress / 100)
                        })
                    })
                }
            }, {
                key: "unZip",
                value: function (e, t, i, n) {
                    this.fileSystemManager.unzip({
                        zipFilePath: e,
                        targetPath: ks.env.USER_DATA_PATH + "/" + t,
                        success: i,
                        fail: n
                    })
                }
            }, {
                key: "checkFileAccess",
                value: function (e, t) {
