                }
            }, {
                key: "updatePercent",
                value: function () {
                    var e = this.percent - this.curPercent,
                        t = e / Math.abs(e);
                    t *= this.updateValue, this.curPercent > this.percent ? this.curPercent = Math.max(this.curPercent + t, this.percent) : this.curPercent = Math.min(this.curPercent + t, this.percent)
                }
            }, {
                key: "updateWidth",
                value: function () {
                    var e = Math.min(1, this.curPercent);
                    e = Math.max(0, e), this.width = e * this.baseWidth
                }
            }]), t
