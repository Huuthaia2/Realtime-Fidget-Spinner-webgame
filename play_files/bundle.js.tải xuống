                            Laya.Vector3.subtract(this.transform.position, this.target.transform.position, e), e = X.changeAngle([e.x, e.z, Math.PI / this.shiftDir]);
                            break;
                        case Ce.Skill:
                            e = new Laya.Vector3;
                        default:
                            e = this.dir
                    }
                    return e || (e = new Laya.Vector3), Laya.Vector3.normalize(e, e), this.dir = e, !0
                }
