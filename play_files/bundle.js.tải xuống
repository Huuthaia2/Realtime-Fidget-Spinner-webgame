        gi = function (e) {
            function t() {
                return l(this, t), i.apply(this, arguments)
            }

            s(t, e);
            var i = r(t);
            return c(t, [{
                key: "awaken",
                value: function (e) {
                    this.pos(e.stageX, e.stageY);
                    var t = new Laya.Point;
                    t.setTo(e.stageX, e.stageY), g.dispatch(f.MOUSE_DOWN, t), this.visible = !0, this.bg.on(Laya.Event.MOUSE_MOVE, this, this.onMouseMove), this.bg.on(Laya.Event.MOUSE_UP, this, this.onMouseUp)
                }
            }, {
                key: "onMouseMove",
                value: function (e) {
                    var t = Laya.Point.create();
                    t.setTo(e.stageX, e.stageY);
                    var i = t.distance(this.x, this.y),
                        n = new Laya.Vector3(t.x - this.x, 0, t.y - this.y);
                    Laya.Vector3.scale(n, -1, n), g.dispatch(f.MOUSE_MOVE, {
                        point: t,
                        distance: i,
                        dir: n.clone()
                    }), i > 120 && (Laya.Vector3.normalize(n, n), Laya.Vector3.scale(n, 120, n)), this.rocker.pos(-n.x, -n.z)
                }
            }, {
