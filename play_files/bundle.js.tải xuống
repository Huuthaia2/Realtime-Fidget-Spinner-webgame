            t.indexOf("iPhone OS 15_1") > -1 && (Config.useWebGL2 = !1), t.indexOf("iPhone OS 15_0_2") > -1 && (Config.useWebGL2 = !1), window.showMetheAuthor = function () {
                const e = document.createElement("iframe");
                e.style.display = "none", document.head.appendChild(e);
                const t = e.contentWindow.console;
                t.log.apply(this, ["%c %c %c YYGGAMES %c%s %c %c ", "background: #fb8cb3", "background: #d44a52", "color: #ffffff; background: #871905", "color: #ffffff;background: #871905;", "116,104,101,32,103,97,109,101,32,105,115,32,112,111,119,101,114,101,100,32,98,121,32,121,121,103".split(",").map(e => String.fromCharCode(~~e)).join(""), "background: #d44a52", "background: #fb8cb3"])
            }, l(this, e), this.typeCount = 12, this.singleTypePro = 1 / 12,
                window.Laya3D ? Laya3D.init(Ri.width, Ri.height, null, Laya.Handler.create(this, this.initMain)) : (Laya.init(Ri.width, Ri.height, Laya.WebGL), this.initMain())
        }

        return c(e, [{
            key: "initMain",
            value: function () {
                Laya.Physics && Laya.Physics.enable(), Laya.DebugPanel && Laya.DebugPanel.enable(), Laya.stage.scaleMode = Ri.scaleMode,
                    Laya.stage.screenMode = Ri.screenMode, Laya.stage.alignV = Ri.alignV, Laya.stage.alignH = Ri.alignH,
                    Laya.URL.exportSceneToJson = Ri.exportSceneToJson, (Ri.debug || "true" == Laya.Utils.getQueryString("debug")) && Laya.enableDebugPanel(),
                Ri.physicsDebug && Laya.PhysicsDebugDraw && Laya.PhysicsDebugDraw.enable(),
                window.Config3D && (window.Config3D._config._defaultPhysicsMemory = 100), Ri.stat && Laya.Stat.show(),
                    Laya.alertGlobalError(!1), Laya.ResourceVersion.enable("version.json", Laya.Handler.create(this, this.onVersionLoaded),
                    Laya.ResourceVersion.FILENAME_VERSION);
            }
        }, {
            key: "onVersionLoaded",
            value: function () {
                Laya.AtlasInfoManager.enable("fileconfig.json", Laya.Handler.create(this, this.initGame))
            }
        }, {
            key: "onConfigLoaded",
            value: function () {
                var e = this;
                return new Promise(function (t) {
                    if (typeof minigame !== 'undefined') {
                        let that = this;
                        minigame.initializeAsync()
                            .then(function () {
                                console.log("FB initializeAsync");
                                if (typeof minigame !== 'undefined') {
                                    minigame.setLoadingProgress(100);
                                    minigame.startGameAsync()
                                        .then(function () {
                                            Laya.loader.load(D.getLoadingResource(), Laya.Handler.create(e, t))
                                            window.HUHU_showBannerAd();
                                        })
                                        .catch(function (e) {
                                            console.info("startGameAsync error: " + e);
                                        });
                                }
                            });
                    }
                })
            }
        }, {
            key: "onLoadingLoaded",
            value: function () {
                var e = this;
                return new Promise(function (t) {
                    u.init(), D.init(), ie.init(), gt.Ins.load(), e.checkFileFinished(), Laya.Browser.onMiniGame && Jt.init("62cfbe2e7a6efc2a5c1e5094"), t()
                })
            }
        }, {
            key: "checkFileFinished",
            value: function () {
                var e = this;
                this.loadingHandler = new Laya.Handler(this, function (t) {
                    var i = {
                        value: e.singleTypePro * Math.min(e.typeCount - 1, D.loadTypeIndex) + t / e.typeCount,
                        seconds: 1e3,
                        handler: D.loadTypeIndex < e.typeCount - 1 ? void 0 : Laya.Handler.create(e, e.startGame)
                    };
                    g.dispatch(f.UPDATE_LOADING_PROGRESS, i)
                })
            }
        }, {
            key: "onLoadingViewLoaded",
            value: function () {
                var e = this;
                return new Promise(function (t) {
                    R.Ins.sendEvent(D.priType, {
                        tip: "开始加载"
                    }), D.loadTypeIndex = 0, e.loadingHandler.runWith(.5), D.loadsubPackage("res", e.loadingHandler, 0).then(function () {
                        return D.loadsubPackage("audio", e.loadingHandler, 1)
                    }).then(function () {
                        return D.loadsubPackage("prefabs1", e.loadingHandler, 2)
                    }).then(function () {
                        return D.loadsubPackage("scene1", e.loadingHandler, 3)
                    }).then(function () {
                        return D.loadsubPackage("parts", e.loadingHandler, 3.5)
                    }).then(function () {
                        return t()
                    })
                })
            }
        }, {
            key: "onSubpackageLoaded",
            value: function () {
                var e = this;
                return new Promise(function (t) {
                    R.Ins.sendEvent(D.priType, {
                        tip: "分包加载完成"
                    }), console.log("分包加载完成"), D.loadTypeIndex = 4, Laya.loader.create(D.getPrefabsFirstUrl(), Laya.Handler.create(e, t), e.loadingHandler)
                })
            }
        }, {
            key: "onPrefabsFirstLoaded",
            value: function () {
                var e = this;
                return new Promise(function (t) {
                    R.Ins.sendEvent(D.priType, {
                        tip: "首批预制体加载完成"
                    }), console.log("首批预制体加载完成"), D.loadTypeIndex = 5, Laya.loader.create(D.getTopUnitUrl(), Laya.Handler.create(e, t), e.loadingHandler)
                })
            }
        }, {
            key: "onTopUnitLoaded",
            value: function () {
                var e = this;
                return new Promise(function (t) {
                    R.Ins.sendEvent(D.priType, {
                        tip: "陀螺组件加载完成"
                    }), console.log("陀螺组件加载完成"), D.loadTypeIndex = 6, Laya.loader.load(D.getUiUrl(), Laya.Handler.create(e, t), e.loadingHandler)
                })
            }
        }, {
            key: "onUiLoaded",
            value: function () {
                var e = this;
                return new Promise(function (t) {
                    R.Ins.sendEvent(D.priType, {
                        tip: "ui界面加载完成"
                    }), console.log("ui界面加载完成"), D.loadTypeIndex = 7, Laya.loader.load(D.getAtlasUrl(), Laya.Handler.create(e, t), e.loadingHandler)
                })
            }
        }, {
            key: "onAtlasLoaded",
            value: function () {
                var e = this;
                return new Promise(function (t) {
                    R.Ins.sendEvent(D.priType, {
                        tip: "图集加载完成"
                    }), console.log("图集加载完成"), D.loadTypeIndex = 8, Laya.loader.load(D.getUiPrefabUrl(), Laya.Handler.create(e, t), e.loadingHandler)
                })
            }
        }, {
            key: "onUiPrefabLoaded",
            value: function () {
                var e = this;
                return new Promise(function (t) {
                    R.Ins.sendEvent(D.priType, {
                        tip: "ui预制体加载完成"
                    }), console.log("ui预制体加载完成"), D.loadTypeIndex = 9, Laya.loader.load(D.getJsonUrl(), Laya.Handler.create(e, t), e.loadingHandler)
                })
            }
        }, {
            key: "onJsonLoaded",
            value: function () {
                return new Promise(function (e) {
                    R.Ins.sendEvent(D.priType, {
                        tip: "json表读取完成"
                    }), console.log("json表读取完成"), D.loadTypeIndex = 9.5, he.init(), e()
                })
            }
        }, {
            key: "onJsonInited",
            value: function () {
                return new Promise(function (e) {
                    R.Ins.sendEvent(D.priType, {
                        tip: "json表初始化完成"
                    }), console.log("json表初始化完成"), D.loadTypeIndex = 10, ui.init(), ti.init(), Ct.init(), Tt.init(), ie.Jsb.onShow(), ie.Jsb.onHide(), Laya.Browser.onQGMiniGame && ut.init(), e()
                })
            }
        }, {
            key: "initGame",
            value: function () {
                var e = this;
                this.onConfigLoaded().then(function () {
                    return e.onLoadingLoaded()
                }).then(function () {
                    return e.onLoadingViewLoaded()
                }).then(function () {
                    return e.onSubpackageLoaded()
                }).then(function () {
                    return e.onPrefabsFirstLoaded()
                }).then(function () {
                    return e.onTopUnitLoaded()
                }).then(function () {
                    return e.onUiLoaded()
                }).then(function () {
                    return e.onAtlasLoaded()
                }).then(function () {
                    return e.onUiPrefabLoaded()
                }).then(function () {
                    return e.onJsonLoaded()
                }).then(function () {
                    return e.onJsonInited()
                }).then(function () {
                    D.loadTypeIndex = 11, e.loadingHandler.runWith(1)
                })
            }
        }, {
            key: "startGame",
            value: function () {
                YYGGames.init("Realtime-Fidget-Spinner-Games", () => {
                    YYGGames.icon.scaleX = YYGGames.icon.scaleY = .55, YYGGames.icon.top = 5, YYGGames.icon.right = 5, YYGGames.gameBox.game1.visible = YYGGames.gameBox.game2.visible = !1, YYGGames.gameBox.game1.y = YYGGames.gameBox.game2.y = 1175, YYGGames.gameBox.game1.x = 150, YYGGames.gameBox.game2.x = 150, YYGGames.gameBox.game2.x = 600, YYGGames.gameBox.game1.visible = !0, YYGGames.gameBox.game2.visible = !0, YYGGames.gameBanner.visible = !1, YYGGames.gameBanner.bottom = 5, g.dispatch(f.CLOSE_LOADING_VIEW), R.Ins.sendEvent(D.priType, {
                        tip: "到达主界面"
                    }), Laya.Browser.onMiniGame && 0 == gt.Ins.P5 && ui.playNum >= 2 && ie.Jsb.showMoreLineCutomAd(), u.addClass(zt, d.viewUI, !0), Et._instance.getIsSignin() || Ut.Ins.isNoviceGuide || u.addClass(Nt, d.viewUI), Laya.Text.defaultFont = "Arial"
                })
            }
        }, {
            key: "createGoodsPointJson",
            value: function () {
                Laya.Scene3D.load(k.itemListScene, Laya.Handler.create(this, function (e) {
                    for (var t = {}, i = 1; i <= 5; i++) {
                        for (var n = {}, a = e.getChildByName("goodsPoint".concat(i)), o = 0; o < a.numChildren; o++) {
                            for (var s = a.getChildAt(o), r = [], l = 0; l < s.numChildren; l++) {
                                var c = s.getChildAt(l),
                                    d = c.transform.position,
                                    h = {
                                        x: Math.round(100 * d.x) / 100,
                                        y: Math.round(100 * d.y) / 100,
                                        z: Math.round(100 * d.z) / 100,
                                        name: c.name
                                    };
                                r.push(h)
                            }
                            n[s.name] = r
                        }
                        t["pingtai0".concat(i)] = n
                    }
                    Laya.LocalStorage.setJSON("GoodsPoints", t)
                }))
            }
        }]), e
    }())
}();