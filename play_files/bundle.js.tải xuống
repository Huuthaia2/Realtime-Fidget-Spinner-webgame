    }, Q.angle = function (e, t) {
        var i = Math.atan2(e.z, e.x);
        i = 180 * i / Math.PI;
        var n, a = Math.atan2(t.z, t.x);
        return i * (a = 180 * a / Math.PI) >= 0 ? n = a - i : (n = Math.abs(i) + Math.abs(a), i > a && (n *= -1), n > 180 && (n = 360 - n), n < -180 && (n = 360 + n)), n
