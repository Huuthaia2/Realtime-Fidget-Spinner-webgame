                    return isNaN(this._playNum) && (this._playNum = +Laya.LocalStorage.getItem("playNum") || 0), this._playNum
                },
                set: function (e) {
                    isNaN(this._playNum + e) ? console.error("对局局数添加失误。") : (this._playNum = e, Laya.LocalStorage.setItem("playNum", this._playNum + ""))
                }
            }, {
                key: "initResourceVersion",
                value: function () {
                    var e = Laya.loader.getRes("json/gameVersion.json");
                    console.log(e ? "以获取游戏版本脚本" : "未获取游戏版本脚本"), Laya.Browser.onMiniGame ? this.Version = e.wx : Laya.Browser.onQGMiniGame ? this.Version = e.oppo : Laya.Browser.onVVMiniGame ? this.Version = e.vivo : ie.isKuaiShou ? this.Version = e.kuaiShou : ie.isAPK || ie.isMiAPK ? this.Version = "0.0.0" : Laya.Browser.onQQMiniGame ? this.Version = e.qq : this.Version = "0.0.0", this.ResourceVersion = Laya.LocalStorage.getJSON("ResourceVersion") || {}
                }
            }, {
                key: "setResourceVersion",
                value: function (e, t) {
                    this.ResourceVersion[e] = t, Laya.LocalStorage.setJSON("ResourceVersion", this.ResourceVersion)
                }
            }, {
                key: "init",
                value: function () {
                    this.initDevelopLevel(), this.initSeasonTime(), this.initSeasonRewardInfo(), this.initPlayerItemInfo(), this.initLuckOpenInfo(), this.initTaskInfo(), this.initOwnerItemInfo(), Laya.timer.frameLoop(1, this, this.updateOnlineTime)
                }
            }, {
                key: "initDevelopLevel",
                value: function () {
                    var e = Laya.LocalStorage.getJSON("develop_info");
                    if (he.initDevelopInfos(), e) this.developLevelInfo = e;
                    else {
                        for (var t in this.developLevelInfo = he.developInfo, this.developLevelInfo) this.developLevelInfo[t].curLevel = 1, this.developLevelInfo[t].curClass = 0, this.developLevelInfo[t].isUnLock = !1, this.developLevelInfo[t].isUse = !1, +t != ti.originAtk && +t != ti.originAxis && +t != ti.originControl || (this.developLevelInfo[t].isUnLock = !0, this.developLevelInfo[t].itemNum = 1, this.developLevelInfo[t].isUse = !0);
                        Laya.LocalStorage.setJSON("develop_info", this.developLevelInfo)
                    }
                }
            }, {
                key: "getDevelopInfoByType",
                value: function (e) {
                    var t = [];
                    for (var i in this.developLevelInfo) this.developLevelInfo[i].type == e && t.push(this.developLevelInfo[i]);
                    return t
                }
            }, {
                key: "getPowerByItem",
                value: function (e) {
                    var t = he.developInfo[e.id].attribute[e.curClass][rt.blood],
                        i = he.developInfo[e.id].attribute[e.curClass][rt.ack],
                        n = he.developInfo[e.id].attribute[e.curClass][rt.def];
                    return 2 == e.type ? i += he.developInfo[e.id].add_attribute[e.curLevel - 2] || 0 : 3 == e.type ? n += he.developInfo[e.id].add_attribute[e.curLevel - 2] || 0 : 4 == e.type && (t = he.developInfo[e.id].add_attribute[e.curLevel - 2] || 0), t * ti.hpScore + i * ti.atkScore + n * ti.defScore
                }
            }, {
                key: "getHighestPowerItem",
                value: function (e) {
                    for (var t = this, i = this.getDevelopInfoByType(e), n = [], a = 0; a < i.length; a++) i[a].isUnLock && n.push(i[a]);
                    return n.sort(function (e, i) {
                        return t.getPowerByItem(e) > t.getPowerByItem(i) ? -1 : 1
                    }), n[0]
                }
            }, {
                key: "addDevelop",
                value: function (e, t) {
                    for (var i in this.developLevelInfo)
                        if (t == ii.Use && this.developLevelInfo[i].type == this.developLevelInfo[e].type && (this.developLevelInfo[i].isUse = !1), +i == e)
                            if (t == ii.level) this.developLevelInfo[i].curLevel++;
                            else if (t == ii.class) this.developLevelInfo[i].curClass++, this.developLevelInfo[i].quality++;
                            else if (t == ii.Unlock) this.developLevelInfo[i].isUnLock = !0;
                            else {
                                var n = 0;
                                this.developLevelInfo[i].isUse = !0, 2 == this.developLevelInfo[i].type && (n = this.getPowerByItem(this.developLevelInfo[i]) - this.getPowerByItem(this.playerItemInfo.ackItem), this.playerItemInfo.ackItem = this.developLevelInfo[i]), 3 == this.developLevelInfo[i].type && (n = this.getPowerByItem(this.developLevelInfo[i]) - this.getPowerByItem(this.playerItemInfo.gravityItem), this.playerItemInfo.gravityItem = this.developLevelInfo[i]), 4 == this.developLevelInfo[i].type && (n = this.getPowerByItem(this.developLevelInfo[i]) - this.getPowerByItem(this.playerItemInfo.powerItem), this.playerItemInfo.powerItem = this.developLevelInfo[i]), n > 0 && u.addClass(hi, d.viewUI, !1, Math.ceil(n)), Laya.LocalStorage.setJSON("player_item", this.playerItemInfo)
                            }
                    Laya.LocalStorage.setJSON("develop_info", this.developLevelInfo)
                }
            }, {
