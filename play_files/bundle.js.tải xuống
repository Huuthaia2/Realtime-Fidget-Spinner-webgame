        return console.log(t), t
    }, Q.getRandomValue = function (e, t, i) {
        if (!e && 0 != e) return Math.random();
        var n = Math.pow(10, e);
        if (0 == e && (n = 1), !t && 0 != t || !i && 0 != i) return Math.round(Math.random() * n) / n;
        var a = i - t;
        return t + Math.round(a * Math.random() * n) / n
    }, Q.rcoverTween = function (e) {
