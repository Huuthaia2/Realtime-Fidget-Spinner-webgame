        Ke = function (e) {
            function t() {
                var e;
                return l(this, t), (e = i.apply(this, arguments)).shiftDir = 0, e.isSpecialAction = !1, e.isStopMoveBySkill = !1, e.specialTime = 0, e
            }

            s(t, e);
            var i = r(t);
            return c(t, [{
                key: "init",
                value: function (e) {
