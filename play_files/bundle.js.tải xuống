                }
            }, {
                key: "def",
                get: function () {
                    return (this.warInfo.curDef + this.getAttributeAdd(Ne.def)) * this.getAttributeMultiply(Ne.defPercent)
                }
            }, {
                key: "ct",
                get: function () {
                    return this.playerInfo.playerTopInfo.ct + this.getAttributeAdd(Ne.ct)
                }
